// ...

std::ifstream f(&quot;example.json&quot;);
json data = json::parse(f);"><pre>#<span class="pl-k">include</span> <span class="pl-s"><span class="pl-pds">&lt;</span>fstream<span class="pl-pds">&gt;</span></span>
